{
    "name": "awakit/jonlil-ckfinder-bundle",
    "type": "symfony-bundle",
    "description": "fork to keep 5.3 compatibilty and 2.3.1 jonlil/ckfinder",
    "keywords": ["CKFinder", "CKEditor"],
    "authors": [
        {
            "name": "Jonas Liljestrand",
            "email": "jonas.liljestrand@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1",
        "symfony/assetic-bundle": ">=2.1",
        "egeloen/ckeditor-bundle": "2.2.*@dev",
        "awakit/jonlil-ckfinder": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/validator": "*",
        "symfony/twig-bundle": ">=2.1",
        "symfony/browser-kit":  ">=2.3,<2.4-dev",
        "symfony/css-selector": "~2.3"
    },
    "autoload": {
        "psr-0": { "Jonlil\\CKFinderBundle": "" }
    },
    "target-dir": "Jonlil/CKFinderBundle"
}
